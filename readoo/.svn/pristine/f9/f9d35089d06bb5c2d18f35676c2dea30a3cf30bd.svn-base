	padding: 5px 10px;
    margin: 0;
}

.bottom_Download .logo{
	float: left;
    margin-right: 10px
}

.bottom_Download .logo img{
	width: 50px;
    height: 50px;
}

.bottom_Download .B{
	font-size:18px;
	font-family: Arial, Helvetica, sans-serif;
	float: left;
    word-break: break-all;
    word-wrap: break-word										 
}

.bottom_Download .B p{
